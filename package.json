{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently 'bun routes:watch' 'bun --hot --port 5000 src/index.ts'",
    "start": "http-server --port 5000 dist",
    "build": "bun routes:generate && bun run build.ts",
    "lint": "concurrently 'eslint .' 'tsc --noEmit'",
    "routes:generate": "bunx tsr generate",
    "routes:watch": "bunx tsr watch"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-router": "^1.145.7",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.545.0",
    "posthog-js": "^1.314.0",
    "react": "^19",
    "react-dom": "^19",
    "simple-icons": "^16.4.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tanstack/router-cli": "^1.145.7",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "http-server": "^14.1.1",
    "jiti": "^2.6.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0",
    "typescript-eslint": "^8.51.0"
  }
}
